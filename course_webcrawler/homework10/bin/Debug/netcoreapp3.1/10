    window.onload = function () {
        var slideWrapperSelector = '.blog-post-';

        function populateBlogPosts(response) {
            var posts = JSON.parse(response);
            var dateString = '';
            var dateWork = '';
            var postUrl = '';
            var postTitle = '';

            for (var i = 0; i < 3; i++) {
                postUrl = posts[i].Url;
                postTitle = posts[i].title;
                dateWork = new Date(posts[i].PublishedDate);

                if (postTitle.length > 45) {
                    postTitle = postTitle.substring(0, 45) + '...';
                }

                dateString = dateWork.toDateString();
                dateString = dateString.substring(dateString.indexOf(' '), dateString.length);

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-title').innerHTML = '<a data-bi-area="What’s new" href="' + postUrl + '" class="vscom-single-card-title-link" target="_blank">' + postTitle + '</a>';
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-body').innerHTML = posts[i].summary.substring(0, 135) + '...';
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link').href = postUrl;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-subtitle').innerHTML = posts[i].Author;
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-icon img').src = posts[i].AuthorImage;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link img').src = posts[i].FeaturedImage;
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link img').alt = postTitle;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-footer').innerHTML = dateString;
            }
          if (jQuery(".vscom-single-box-link").length > 0) {
            jQuery(".vscom-single-box-link")
            .attr("data-bi-area", "What’s new");
          }
        }

        var xhr = new XMLHttpRequest();

        xhr.open('GET', 'https://visualstudio.microsoft.com/wp-content/plugins/vscom-utility-plugin/json/home-devblogs.json');
        xhr.setRequestHeader('Content-Type', 'application/json');

        xhr.onreadystatechange = function() {
            if (xhr.readyState === 4) {
                if (xhr.status === 200) {
                    populateBlogPosts(xhr.response);
                } else {
                    /* error occurred */
                    console.log(xhr);
                }
            }
        }

        xhr.send();
    }
</script>

