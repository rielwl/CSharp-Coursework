</div><div class="fusion-clearfix"></div></div></div></div></div><div class="fusion-fullwidth fullwidth-box fusion-builder-row-9 vscom-swimlane vscom-swimlane--no-bottom-padding nonhundred-percent-fullwidth non-hundred-percent-height-scrolling" style="background-color: #ffffff;background-position: center center;background-repeat: no-repeat;padding-top:0px;padding-right:12px;padding-bottom:0px;padding-left:12px;margin-bottom: 0px;margin-top: 0px;border-width: 0px 0px 0px 0px;border-color:#eae9e9;border-style:solid;" ><div class="fusion-builder-row fusion-row"><div class="fusion-layout-column fusion_builder_column fusion-builder-column-15 fusion_builder_column_1_1 1_1 fusion-one-full fusion-column-first fusion-column-last" style="margin-top:15px;margin-bottom:15px;"><div class="fusion-column-wrapper fusion-flex-column-wrapper-legacy" style="background-position:left top;background-repeat:no-repeat;-webkit-background-size:cover;-moz-background-size:cover;-o-background-size:cover;background-size:cover;padding: 0px 0px 0px 0px;"><style type="text/css"></style><div class="fusion-title title fusion-title-7 fusion-sep-none fusion-title-text fusion-title-size-two" style="font-size:50px;margin-top:15px;margin-right:0px;margin-bottom:15px;margin-left:0px;"><h2 class="title-heading-left fusion-responsive-typography-calculated" style="margin:0;font-size:1em;--fontSize:50;line-height:1.2;">What&#8217;s new from the Visual Studio team</h2></div> <script type="text/javascript">
    window.onload = function () {
        var slideWrapperSelector = '.blog-post-';

        function populateBlogPosts(response) {
            var posts = JSON.parse(response);
            var dateString = '';
            var dateWork = '';
            var postUrl = '';
            var postTitle = '';

            for (var i = 0; i < 3; i++) {
                postUrl = posts[i].Url;
                postTitle = posts[i].title;
                dateWork = new Date(posts[i].PublishedDate);

                if (postTitle.length > 45) {
                    postTitle = postTitle.substring(0, 45) + '...';
                }

                dateString = dateWork.toDateString();
                dateString = dateString.substring(dateString.indexOf(' '), dateString.length);

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-title').innerHTML = '<a data-bi-area="What’s new" href="' + postUrl + '" class="vscom-single-card-title-link" target="_blank">' + postTitle + '</a>';
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-body').innerHTML = posts[i].summary.substring(0, 135) + '...';
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link').href = postUrl;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-subtitle').innerHTML = posts[i].Author;
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-icon img').src = posts[i].AuthorImage;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link img').src = posts[i].FeaturedImage;
                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-link img').alt = postTitle;

                document.querySelector(slideWrapperSelector + i + ' .vscom-single-box-footer').innerHTML = dateString;
            }
          if (jQuery(".vscom-single-box-link").length > 0) {
            jQuery(".vscom-single-box-link")
            .attr("data-bi-area", "What’s new");
          }
        }

        var xhr = new XMLHttpRequest();

        xhr.open('GET', 'https://visualstudio.microsoft.com/wp-content/plugins/vscom-utility-plugin/json/home-devblogs.json');
        xhr.setRequestHeader('Content-Type', 'application/json');

        xhr.onreadystatechange = function() {
            if (xhr.readyState === 4) {
                if (xhr.status === 200) {
                    populateBlogPosts(xhr.response);
                } else {
                    /* error occurred */
                    console.log(xhr);
                }
            }
        }

        xhr.send();
    }
</script>

